{
    "files.exclude": {
        "**/.vs/": true,
        "**/__pycache__": true,
        "**/.cache": true,
        "**/.pytest_cache": true,
        ".pytest_cache/**/*": true
    },
    "python.pythonPath": "${env:HOME}/.pyenv/versions/scripts/bin/python",
    "python.testing.pytestArgs": [
        "-v"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true
}
